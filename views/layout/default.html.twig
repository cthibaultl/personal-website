<!DOCTYPE html>
<!--[if lt IE 7]>      <html lang="fr" class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html lang="fr" class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html lang="fr" class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html lang="fr" class="no-js"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block title %}{% endblock %}</title>
        {% block meta %}{% endblock %}
        <meta name="viewport" content="width=device-width">
        <link href='http://fonts.googleapis.com/css?family=Abel|Muli:300,400,300italic,400italic' rel='stylesheet' type='text/css'>
        <link rel="stylesheet" href="css/stylesheets/main.css">
        <script src="js/vendor/modernizr-2.6.1.min.js"></script>
    </head>
    <body class="{% block body %}{% endblock %}">
    {% spaceless %}
        <header>
            <a href="#" class="skip visuallyhidden focusable">Passer au contenu</a>
            <h1 class="visuallyhidden">Site personnel de Cynthia Thibault-Larouche</h1>
            <nav role="navigation">
                <h2 class="visuallyhidden">Navigation principale</h2>
                <ul class="mainnav">
                    <li>
                        <a href="/#about">À propos</a>
                    </li>
                    <li>
                        <a href="/#works">Réalisations</a>
                    </li>
                    <li>
                        <a href="/#contact">Contact</a>
                    </li>
                </ul>
                <h2 class="visuallyhidden">Navigation secondaire</h2>
                <ul class="secondarynav">
                    <li>
                        <a href="accessibilite">Accessibilité</a>
                    </li>
                </ul>
            </nav>
        </header>
        
        <div id="main" role="main">
            {% block content %}
            {% endblock %}
        </div>
        
        <footer>
            <div class="footer-wrap clearfix" id="contact">
                <ul>
                    <li class="email">
                        <a href="mailto:contact@cthibaultl.com"><span class="visuallyhidden" lang="en">Email</span></a>
                    </li>
                    <li class="twitter">
                        <a href="https://twitter.com/cthibaultl"><span class="visuallyhidden" lang="en">Twitter</span></a>
                    </li>
                    <li class="linkedin">
                        <a href="http://ca.linkedin.com/pub/cynthia-thibault-larouche/30/489/845"><span class="visuallyhidden" lang="en">LinkedIn</span></a>
                    </li>
                </ul>
                <p role="contentinfo">
                    © Tous droits réservés cthibaultl.com
                </p>
            </div>
        </footer>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.8.0.min.js"><\/script>')</script>
        <script src="js/vendor/jquery.imagesloaded.min.js"></script>
        <script src="js/vendor/jquery.isotope.min.js"></script>
        <script src="js/vendor/raphael-min.js"></script>
        <script src="js/vendor/jquery.scrollto.min.js"></script>
	{% if app['debug'] == false %} {# production assets #}
		<script src="js/app.min.js"></script>
	    <script>
	
		  var _gaq = _gaq || [];
		  _gaq.push(['_setAccount', 'UA-39352848-1']);
		  _gaq.push(['_trackPageview']);
		
		  (function() {
		    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
		    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
		    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
		  })();
		
		</script>
	{% else %} {# development assets #}
	    <script src="js/plugins.js"></script>
	    <script src="js/libs/chart.js"></script>
	    <script src="js/libs/works.js"></script>
	    <script src="js/libs/axs.js"></script>
	    <script src="js/main.js"></script>
	{% endif %}

        <span id="arialive" class="visuallyhidden" aria-live="polite"></span>
    {% endspaceless %}
    </body>
</html>
